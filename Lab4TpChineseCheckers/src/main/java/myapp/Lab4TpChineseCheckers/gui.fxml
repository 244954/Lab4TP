<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="main_board" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myapp.Lab4TpChineseCheckers.GUIController">
   <children>
      <Button layoutX="27.0" layoutY="24.0" mnemonicParsing="false" onAction="#new_game" prefHeight="25.0" prefWidth="50.0" text="Start" />
      <Pane fx:id="board" layoutX="27.0" layoutY="63.0" prefHeight="500.0" prefWidth="500.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Pane>
      <Button layoutX="144.0" layoutY="24.0" mnemonicParsing="false" onAction="#endmove" text="End of move" />
      <Button layoutX="92.0" layoutY="24.0" mnemonicParsing="false" onAction="#info" text="Info" />
      <TextField fx:id="textfield" alignment="CENTER" editable="false" layoutX="28.0" layoutY="563.0" prefHeight="25.0" prefWidth="547.0" />
   </children>
</Pane>
