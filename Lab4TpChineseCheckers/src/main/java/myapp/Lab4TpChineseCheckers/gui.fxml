      <Text layoutX="55.0" layoutY="15.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Rules:" />
      <TextField fx:id="rulesType" layoutX="55.0" layoutY="24.0" prefHeight="25.0" prefWidth="31.0" text="1" />
      <Button layoutX="103.0" layoutY="24.0" mnemonicParsing="false" onAction="#new_game" prefHeight="25.0" prefWidth="50.0" text="Start" />
      <Pane fx:id="board" layoutX="27.0" layoutY="63.0" prefHeight="500.0" prefWidth="500.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Pane>
      <Button layoutX="28.0" layoutY="570.0" mnemonicParsing="false" onAction="#endmove" text="End of move" />
